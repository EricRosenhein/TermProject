package userinterface;

import java.util.Vector;

import javafx.beans.property.SimpleStringProperty;

//==============================================================================

public class TreeTableModel
{
    private final SimpleStringProperty barcode;
    private final SimpleStringProperty notes;

    //----------------------------------------------------------------------------
	public ScoutTableModel(Vector<String> treeData)
	{
		barcode =  new SimpleStringProperty(treeData.elementAt(0));
        notes =  new SimpleStringProperty(notesData.elementAt(1));
	}

    //----------------------------------------------------------------------------
    public String getBarcode() {
        return barcode.get();
    }

    //----------------------------------------------------------------------------
    public String setBarcode(String barcode) {
        barcode.set(barcode);
    }

    //----------------------------------------------------------------------------
    public String getNotes() {
        return notes.get();
    }
    
    //----------------------------------------------------------------------------
    public String setNotes(String notes) {
        notes.set(notes);
    }

}